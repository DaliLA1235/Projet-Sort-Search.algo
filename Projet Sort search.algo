def insertion_sort(arr):
    n = len(arr)
    for i in range(1, n):
        j = i
        while j > 0 and arr[j] < arr[j - 1]:
            arr[j], arr[j - 1] = arr[j - 1], arr[j]
            j -= 1

# Example usage:
my_array = [12, 11, 13, 5, 6]
insertion_sort(my_array)
print("Sorted array:", my_array)